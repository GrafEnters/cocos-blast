{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "..",
    "paths": {
      "*": ["*"]
    }
  },
  "include": [
    "../assets/Script/**/*",
    "**/*"
  ],
  "exclude": [
    "../node_modules",
    "../library",
    "../temp",
    "../build"
  ]
}
